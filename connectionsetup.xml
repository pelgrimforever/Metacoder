<?xml version="1.0" encoding="UTF-8"?>
<databases>
	<postgresql>
		<javaclass>org.postgresql.Driver</javaclass>
		<sqlmapper>db.SQLMapper_pgsql</sqlmapper>
		<connectionprefix>jdbc:</connectionprefix>
		<connectiontool>postgresql://</connectiontool>
		<databaseseparator>/</databaseseparator>
		<default>
			<ip>localhost</ip>
			<port>5432</port>
			<portseparator>:</portseparator>
			<databasename>postgres</databasename>
			<user>postgres</user>
		</default>
	</postgresql>
	<mysql>
		<javaclass>com.mysql.jdbc.Driver</javaclass>
		<sqlmapper>db.SQLMapper_mysql</sqlmapper>
		<connectionprefix>jdbc:</connectionprefix>
		<connectiontool>mysql://</connectiontool>
		<databaseseparator>/</databaseseparator>
		<default>
			<ip>localhost</ip>
			<port>3306</port>
			<portseparator>:</portseparator>
			<databasename></databasename>
			<user></user>
		</default>
	</mysql>
	<oracleXE>
		<javaclass>oracle.jdbc.OracleDriver</javaclass>
		<sqlmapper>db.SQLMapper_oracle</sqlmapper>
		<connectionprefix>jdbc:</connectionprefix>
		<connectiontool>oracle:thin:@</connectiontool>
		<databaseseparator>:</databaseseparator>
		<default>
			<ip>localhost</ip>
			<port>5432</port>
			<portseparator>:</portseparator>
			<databasename>1521</databasename>
			<user>XE</user>
		</default>
	</oracleXE>
	<MSSQL>
		<javaclass>com.microsoft.sqlserver.jdbc.SQLServerDriver</javaclass>
		<sqlmapper>db.SQLMapper_mssql</sqlmapper>
		<connectionprefix>jdbc:</connectionprefix>
		<connectiontool>sqlserver://</connectiontool>
		<databaseseparator>;databaseName=</databaseseparator>
		<default>
			<ip>localhost</ip>
			<port></port>
			<portseparator></portseparator>
			<databasename></databasename>
			<user></user>
		</default>
	</MSSQL>
</databases>
