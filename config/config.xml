<config>
  <server></server>
  <defaultwebservice></defaultwebservice>

  <defaultlanguage>BE_NL</defaultlanguage>
  <translationmap>text/</translationmap>
	<!-- if menuxml has a value, standard menu controls are loaded -->
	<!-- else, the defaultscreen javaclass is loaded -->
  <fxmenuxml></fxmenuxml>
	<defaultscreen>metacoder.GUI.Projectcanvas</defaultscreen>

  <UIsettings>
		<fontname>Serif</fontname>
		<fontsize>10</fontsize>
		<buttonfontsize>10</buttonfontsize>
		<tabfontsize>14</tabfontsize>
		<panelpadding>10</panelpadding>
		<panelarc>5</panelarc>
		<imagearc>3</imagearc>
  </UIsettings>

  <UIelements>
		<waiticon>icons/expand.png</waiticon>
		<expandicon>icons/wait.png</expandicon>
  </UIelements>
    
  <COMPONENT>
    <id>popupmessage</id>
    <WINDOW_TITLE>Message</WINDOW_TITLE>
    <BUTTON_OK>OK</BUTTON_OK>
  </COMPONENT>
</config>
